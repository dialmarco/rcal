var monthly_series = new Array();
<% again.each do |entry| %>
monthly_series.push(new Array(<%= "#{entry[0]}, #{entry[1]}" %>));
<% end %>
$(function () {
  $.plot($("#placeholder"), [{data : monthly_series, bars: {show:true, barWidth: 24*60*60*1000}}], { xaxis: { mode: 'time', timeformat: "%y/%m/%d" } }) ;
});

